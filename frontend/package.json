    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^8.0.2",
    "jsdom": "^20.0.2",
    "lint-staged": "^13.0.3",
    "msw": "^0.49.0",
    "next-router-mock": "^0.8.0",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "sass": "^1.56.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.25.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
