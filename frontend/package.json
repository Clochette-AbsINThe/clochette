    "chart.js": "^4.0.1",
    "jose": "^4.11.0",
    "next": "^13.0.4",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.0.1",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "sharp": "^0.31.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
